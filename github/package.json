{
  "name": "elite-dangerous-next",
  "version": "1.0.0-alpha",
  "type": "module",
  "description": "Мониторинг и обработка журнальных файлов Elite Dangerous в реальном времени",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "dev": "npm run build:client && tsx watch src/index.ts",
    "build": "npm run clean && npx tsc && npm run build:client",
    "build:client": "vite build",
    "clean": "node -e \"const fs=require('fs');if(fs.existsSync('dist'))fs.rmSync('dist',{recursive:true});\"",
    "start": "node dist/index.js",
    "test": "jest --config jest.config.cjs"
  },
  "keywords": [
    "elite-dangerous",
    "assistant",
    "gaming",
    "websocket",
    "elite",
    "journal",
    "monitor"
  ],
  "author": "rewtwrtwr",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@tanstack/react-virtual": "^3.13.18",
    "axios": "^1.13.5",
    "better-sqlite3": "^12.6.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "preact": "^10.28.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "sql.js": "^1.14.0",
    "winston": "^3.19.0",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@tanstack/react-virtual": "^3.13.18",
    "@types/better-sqlite3": "^7.6.12",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.4",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^5.1.4",
    "tsx": "^4.21.0",
    "typescript": "^5.3.3",
    "vite": "^7.3.1"
  }
}
